<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wakemeup"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon_hdpi"
        android:label="@string/app_name"
        android:theme="@style/CustomTheme" >
        <activity
            android:name="com.wakemeup.MainClock"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wakemeup.AlarmList"
            android:label="Alarms" >
        </activity>
        <activity android:name="com.wakemeup.AlarmRecieverActivity" >
        </activity>
        <activity
            android:name="com.wakemeup.QuestionForm"
            android:label="@string/title_activity_question_form" >
        </activity>
        <activity
            android:name="com.wakemeup.MathQuestions"
            android:label="@string/title_activity_math_questions" >
        </activity>
        <activity
            android:name="com.wakemeup.CustomQuestion"
            android:label="@string/title_activity_custom_question" >
        </activity>
    </application>

</manifest>